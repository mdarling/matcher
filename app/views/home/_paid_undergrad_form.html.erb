 <fieldset>
  <div class="field">
    <%= f.label "Is this position posted on UNM jobs?" %><br />
    <%= f.text_field :unmJobs,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "If so, what is the posting number?" %><br />
    <%= f.text_field :postingNumber,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "Position Title" %><br />
    <%= f.text_field :positionTitle,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "Work Period" %><br />
    <%= f.text_field :workPeriod,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "Hours per week" %><br />
    <%= f.text_field :weeklyTimeCommitment,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "Desired Skills" %><br />
    <%= f.text_area :desiredSkills,:class => 'textArea' %>
  </div>
  <div class="field">
    <%= f.label "Desired Classes" %><br />
    <%= f.text_area :desiredClasses,:class => 'textArea' %>
  </div>
  <div class="field">
    <%= f.label "When should this position be posted?" %><br />
    <%= f.date_select :recordBegin %>
  </div>
  <div class="field">
    <%= f.label "What is the application deadline?" %><br />
    <%= f.date_select :recordEnd %>
  </div>
  <div class="field">
    <%= f.label "Minimum GPA" %><br />
    <%= f.text_field :minimumGPA,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "Desired Academic Standing" %><br />
    <%= f.text_field :academicLevel,:class => 'textField' %>
  </div>
  <div class="field">
    <%= f.label "General Job Description" %><br />
    <%= f.text_area :jobDescription,:class => 'textArea' %>
  </div>
  <div class="actions">
  
  </div>
</fieldset>
