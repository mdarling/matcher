<h1>Listing project_profiles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Department</th>
    <th>Email</th>
    <th>Unpaid undergrad</th>
    <th>Unpaid grad</th>
    <th>Paid grad</th>
    <th>Unpaid grad</th>
    <th>Listed</th>
    <th>Posting number</th>
    <th>Position title</th>
    <th>Work period</th>
    <th>Academic level</th>
    <th>Minimum gpa</th>
    <th>Required classes</th>
    <th>Other qualifications</th>
    <th>Job description</th>
    <th>Application deadline</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_profiles.each do |project_profile| %>
  <tr>
    <td><%= project_profile.title %></td>
    <td><%= project_profile.department %></td>
    <td><%= project_profile.email %></td>
    <td><%= project_profile.unpaid_undergrad %></td>
    <td><%= project_profile.unpaid_grad %></td>
    <td><%= project_profile.paid_grad %></td>
    <td><%= project_profile.unpaid_grad %></td>
    <td><%= project_profile.listed %></td>
    <td><%= project_profile.posting_number %></td>
    <td><%= project_profile.position_title %></td>
    <td><%= project_profile.work_period %></td>
    <td><%= project_profile.academic_level %></td>
    <td><%= project_profile.minimum_gpa %></td>
    <td><%= project_profile.required_classes %></td>
    <td><%= project_profile.other_qualifications %></td>
    <td><%= project_profile.job_description %></td>
    <td><%= project_profile.application_deadline %></td>
    <td><%= link_to 'Show', project_profile %></td>
    <td><%= link_to 'Edit', edit_project_profile_path(project_profile) %></td>
    <td><%= link_to 'Destroy', project_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

