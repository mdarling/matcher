<h1>Listing student_profiles</h1>

<table>
  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Unmid</th>
    <th>Email</th>
    <th>Phonenumber</th>
    <th>Academiclevel</th>
    <th>Major</th>
    <th>Minor</th>
    <th>Expectedgraduation</th>
    <th>Interests</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @student_profiles.each do |student_profile| %>
  <tr>
    <td><%= student_profile.firstName %></td>
    <td><%= student_profile.lastName %></td>
    <td><%= student_profile.unmID %></td>
    <td><%= student_profile.email %></td>
    <td><%= student_profile.phoneNumber %></td>
    <td><%= student_profile.academicLevel %></td>
    <td><%= student_profile.major %></td>
    <td><%= student_profile.minor %></td>
    <td><%= student_profile.expectedGraduation %></td>
    <td><%= student_profile.interests %></td>
    <td><%= link_to 'Show', student_profile %></td>
    <td><%= link_to 'Edit', edit_student_profile_path(student_profile) %></td>
    <td><%= link_to 'Destroy', student_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

