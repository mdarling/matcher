<h1>Listing unpaid_grad_positions</h1>

<table>
  <tr>
    <th>Workperiod</th>
    <th>Weeklytimecommitment</th>
    <th>Deliverables</th>
    <th>Desiredskills</th>
    <th>Desiredclasses</th>
    <th>Recordbegin</th>
    <th>Recordend</th>
    <th>Minimumgpa</th>
    <th>Academiclevel</th>
    <th>Jobdescription</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @unpaid_grad_positions.each do |unpaid_grad_position| %>
  <tr>
    <td><%= unpaid_grad_position.workPeriod %></td>
    <td><%= unpaid_grad_position.weeklyTimeCommitment %></td>
    <td><%= unpaid_grad_position.deliverables %></td>
    <td><%= unpaid_grad_position.desiredSkills %></td>
    <td><%= unpaid_grad_position.desiredClasses %></td>
    <td><%= unpaid_grad_position.recordBegin %></td>
    <td><%= unpaid_grad_position.recordEnd %></td>
    <td><%= unpaid_grad_position.minimumGPA %></td>
    <td><%= unpaid_grad_position.academicLevel %></td>
    <td><%= unpaid_grad_position.jobDescription %></td>
    <td><%= link_to 'Show', unpaid_grad_position %></td>
    <td><%= link_to 'Edit', edit_unpaid_grad_position_path(unpaid_grad_position) %></td>
    <td><%= link_to 'Destroy', unpaid_grad_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

