<h1>Listing unpaid_undergrad_positions</h1>

<table>
  <tr>
    <th>Workperiod</th>
    <th>Weeklytimecommitment</th>
    <th>Deliverables</th>
    <th>Desiredskills</th>
    <th>Desiredclasses</th>
    <th>Recordbegin</th>
    <th>Recordend</th>
    <th>Minimumgpa</th>
    <th>Academiclevel</th>
    <th>Jobdescription</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @unpaid_undergrad_positions.each do |unpaid_undergrad_position| %>
  <tr>
    <td><%= unpaid_undergrad_position.workPeriod %></td>
    <td><%= unpaid_undergrad_position.weeklyTimeCommitment %></td>
    <td><%= unpaid_undergrad_position.deliverables %></td>
    <td><%= unpaid_undergrad_position.desiredSkills %></td>
    <td><%= unpaid_undergrad_position.desiredClasses %></td>
    <td><%= unpaid_undergrad_position.recordBegin %></td>
    <td><%= unpaid_undergrad_position.recordEnd %></td>
    <td><%= unpaid_undergrad_position.minimumGPA %></td>
    <td><%= unpaid_undergrad_position.academicLevel %></td>
    <td><%= unpaid_undergrad_position.jobDescription %></td>
    <td><%= link_to 'Show', unpaid_undergrad_position %></td>
    <td><%= link_to 'Edit', edit_unpaid_undergrad_position_path(unpaid_undergrad_position) %></td>
    <td><%= link_to 'Destroy', unpaid_undergrad_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

